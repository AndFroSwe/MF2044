function [ exectime, data ] = act_code( segment, data )
%PID_CODE Summary of this function goes here
%   Detailed explanation goes here

switch segment
    case 1
        r = tt;
        y = ttAnalogIn(data.yChan);
        data = pidcalc(data, r, y);
        
        exectime = data.wcet;
        
    case 2
        ttAnalogOut(data.uChan, data.u);
        exectime = -1;
end

